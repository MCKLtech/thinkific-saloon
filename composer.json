{
  "name": "mckltech/thinkific-saloon",
  "description": "PHP Library for the Thinkific API using Saloon",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/MCKLtech/lms-contracts"
    }
  ],
  "authors": [
    {
      "name": "Colin Longworth",
      "email": "info@wooninja.io"
    }
  ],
  "require": {
    "php": "^8.1",
    "saloonphp/saloon": "^3.0",
    "saloonphp/pagination-plugin": "^2.2",
    "saloonphp/rate-limit-plugin": "^2.0",
    "nesbot/carbon": "*",
    "mckltech/lms-contracts": "^1.0"
  },
  "require-dev": {
    "spatie/ray": "*",
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.6",
    "guzzlehttp/guzzle": "^7.9"
  },
  "autoload": {
    "psr-4": {
      "WooNinja\\ThinkificSaloon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WooNinja\\ThinkificSaloon\\Tests\\": "tests/"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable" : true
}
